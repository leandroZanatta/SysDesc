<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd  http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
	<changeSet id="20191006_141603" author="Leandro Zanatta">

		<preConditions onFail="CONTINUE">
			<not>
				<tableExists tableName="tb_modulogerenciadorpdv" />
			</not>
		</preConditions>

		<createTable tableName="tb_modulogerenciadorpdv">

			<column name="id_modulogerenciadorpdv" type="NUMBER(18,0)" remarks="CÓDIGO INDENTIFICADOR">
				<constraints primaryKey="true" primaryKeyName="modulogerenciadorpdv_pk" nullable="false" />
			</column>

			<column name="cd_gerenciadorpdv" type="NUMBER(18,0)" remarks="CÓDIGO DO GERENCIADOR INICIALIZAÇÃO">
				<constraints nullable="false" foreignKeyName="modulopdv_gerenciadorpdv_fk" references="tb_gerenciadorpdv(id_gerenciadorpdv)" />
			</column>

			<column name="cd_modulopdv" type="NUMBER(18,0)" remarks="CÓDIGO DO MÓDULO">
				<constraints nullable="false" foreignKeyName="modulopdv_modulopdv_fk" references="tb_modulopdv(id_modulopdv)" />
			</column>

			<column name="nr_porta" type="NUMBER(18,0)" remarks="PORTA DO MPODULO">
				<constraints nullable="true" />
			</column>

		</createTable>

		<createSequence sequenceName="gen_modulogerenciadorpdv" />
	</changeSet>
</databaseChangeLog>
